{
  "name": "typeson-registry",
  "version": "1.0.0-alpha.20",
  "description": "The type registry for typeson",
  "author": "dfahlander",
  "contributors": [
    "Brett Zamir"
  ],
  "main": "./dist/all.js",
  "module": "./index.js",
  "scripts": {
    "eslint": "eslint browser-test/ test/ presets/ types/ *.js",
    "test": "npm run eslint && npm run rollup && mocha test/test-node",
    "browser-test": "npm run eslint && npm run rollup && npm start browser-test/",
    "rollup": "node ./build.js",
    "windows": "node windows-devinstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfahlander/typeson-registry.git"
  },
  "keywords": [
    "typeson",
    "JSON",
    "remoting",
    "universal"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfahlander/typeson-registry/issues"
  },
  "homepage": "https://github.com/dfahlander/typeson-registry#readme",
  "engines": {
    "node": "x"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "canvas": "1.6.9",
    "chai": "4.1.2",
    "eslint": "4.18.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "6.0.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "jsdom": "11.6.2",
    "mocha": "5.0.1",
    "node-static": "0.7.10",
    "opn": "5.2.0",
    "rollup": "0.56.2",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-uglify": "3.0.0",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "base64-arraybuffer-es6": "0.3.1",
    "typeson": "5.8.2",
    "uuid": "3.2.1",
    "whatwg-url": "6.4.0"
  },
  "tonicExample": "var Typeson = require('typeson');\nvar TSON = new Typeson().register(require('typeson-registry/dist/presets/builtin'));\n\nTSON.stringify({foo: new Date()}, null, 2);"
}
